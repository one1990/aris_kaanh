<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="True" />
  <param
    name="robot_description"
    textfile="$(find rokae2)/urdf/rokae2.urdf" />
  <param
    name="use_gui"
    value="$(arg gui)" />


  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="state_publisher" />



  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find rokae2)/urdf.rviz" />

  <node
    name="move_Node"
    pkg="rokae2"
    type="move_Node"
    />

</launch>
