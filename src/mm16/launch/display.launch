<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="True" />
  <param
    name="robot_description"
    textfile="$(find mm16)/urdf/mm16.urdf" />
  <param
    name="use_gui"
    value="$(arg gui)" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find mm16)/urdf.rviz" />

  <node
    name="move_Nodes"
    pkg="mm16"
    type="move_Nodes"
    />
</launch>
